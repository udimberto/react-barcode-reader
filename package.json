{
  "name": "react-boleto-reader",
  "version": "1.0.1",
  "description": "Utility to scan, via browser camera, brazilian-banking barcode.",
  "author": "udimberto",
  "license": "MIT",
  "repository": "udimberto/react-boleto-reader",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle-crl --no-compress --format modern,cjs",
    "start": "microbundle-crl watch --no-compress --format modern,cjs",
    "prepare": "npm run build",
    "test": "npm run test:unit test:lint test:build",
    "test:build": "npm run build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && npm install && npm run build",
    "deploy": "gh-pages -d example/build",
    "example": "cd example && HTTPS=true npm start"
  },
  "dependencies": {
    "@ericblade/quagga2": "^1.3.1",
    "broleto": "~0.0.10"
  },
  "peerDependencies": {
    "react": "^16.8.x"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-standard": "^4.0.1",
    "gh-pages": "^5.0.0",
    "microbundle-crl": "^0.13.10",
    "prettier": "^2.0.4",
    "react": "^16.8.x",
    "react-dom": "^16.8.x",
    "react-scripts": "^3.4.1"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "boleto",
    "barcode",
    "react",
    "cÃ³digo de barras"
  ]
}
